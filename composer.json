{
  "name": "larva/laravel-flysystem-oss",
  "description": "This is a Flysystem adapter for the Aliyun OSS.",
  "keywords": [ "flysystem", "aliyun", "oss", "larva" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^6.0 || ^7.0 || ^8.0",
    "league/flysystem": "^1.0",
    "aliyuncs/oss-sdk-php": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Flysystem\\Aliyun\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larva\\Flysystem\\Aliyun\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Flysystem\\Aliyun\\ObjectStorageServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
